/ {
    chosen {
        zephyr,console = &usb_serial;
		zephyr,shell-uart = &usb_serial;
        zephyr,settings-partition = &storage_partition;
        xecut,keypad = &uart0;
        xecut,relay0 = &relay0;
    };
     relay0: relay0 {
            compatible = "relay";
            gpios = <&gpio0 14 GPIO_ACTIVE_HIGH>;
    };

};

&uart0 {
    status = "okay";
    current-speed = <9600>;
};

&usb_serial {
    status = "okay";
};

